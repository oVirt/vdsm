%global _polkitdir %{_datadir}/polkit-1/rules.d
%else
%global _polkitdir %{_localstatedir}/lib/polkit-1/localauthority/10-vendor.d
%endif

# Disable debuginfo package, since vdsm is a meta-package
%global debug_package %{nil}

